{
  "name": "benefit",
  "version": "1.0.3",
  "source": "src/index.js",
  "main": "benefit.js",
  "module": "benefit.m.js",
  "unpkg": "benefit.umd.js",
  "repository": "https://github.com/cdonohue/benefit.git",
  "contributors": [
    "Chad Donohue <chad.donohue@gmail.com>",
    "Eric Clemmons <eric@smarterspam.com>"
  ],
  "description": "5kB utility CSS framework that is compatible with TailwindCSS",
  "license": "MIT",
  "keywords": [
    "atomic",
    "css",
    "css-in-js",
    "design",
    "emotion",
    "jsx",
    "system",
    "tailwind",
    "tailwindcss",
    "utility-classes"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "microbundle --entry src/index.js --entry src/util.js --entry src/react.js --output dist --external react,@emotion --jsx React.createElement",
    "postbuild": "./scripts/css.js",
    "prestart": "yarn build",
    "start": "docz dev",
    "predocs:build": "yarn build",
    "docs:build": "docz build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prerelease": "yarn build",
    "postversion": "cp -rf package.json dist && cp -rf README.md dist && cp -rf LICENSE dist && cp benefit-logo.svg dist",
    "release": "np --contents dist",
    "pretest": "yarn build",
    "test": "jest"
  },
  "sideEffects": false,
  "devDependencies": {
    "@emotion/core": "^10.0.10",
    "@emotion/styled": "^10.0.10",
    "babel-eslint": "~10.0.1",
    "babel-preset-react-app": "^8.0.0",
    "docz": "~1.0.4",
    "emotion": "~10.0.9",
    "eslint": "~5.14.1",
    "eslint-config-airbnb": "~17.1.0",
    "eslint-config-prettier": "~4.1.0",
    "eslint-plugin-html": "~5.0.3",
    "eslint-plugin-import": "~2.16.0",
    "eslint-plugin-jsx-a11y": "~6.2.1",
    "eslint-plugin-prettier": "~3.0.1",
    "eslint-plugin-react": "~7.12.4",
    "eslint-plugin-react-hooks": "~1.3.0",
    "jest": "^24.8.0",
    "jest-puppeteer": "^4.1.1",
    "microbundle": "~0.9.0",
    "np": "~4.0.2",
    "parcel-bundler": "^1.12.3",
    "polychrome": "~4.1.2",
    "prettier": "~1.16.4",
    "prop-types": "^15.7.2",
    "puppeteer": "^1.16.0",
    "react": "~16.8.6",
    "react-dom": "~16.8.6",
    "react-feather": "~1.1.6",
    "react-live": "~2.0.0",
    "tailwindcss": "^1.0.1"
  },
  "peerDependencies": {
    "emotion": "~10.0.0"
  },
  "dependencies": {}
}
